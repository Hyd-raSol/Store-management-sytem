@model IEnumerable<string>

@{
    Layout = null;
}
@Html.ActionLink("Home", "List", "Product")
@foreach (string link in Model)
{
    @Html.RouteLink(link.ToString(), new
{
    controller = "Product",
    action = "List",
    category = link,
    page = 1
},
        new
        {
            @class = link.Equals(ViewBag.SelectedCategory) ? "selected" : null
        }
    );
}
